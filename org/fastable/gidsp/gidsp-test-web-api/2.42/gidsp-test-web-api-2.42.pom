            <ignoredNonTestScopedDependency>org.fastable.gidsp:gidsp-service-metadata-workflow</ignoredNonTestScopedDependency>
            <ignoredNonTestScopedDependency>org.fastable.gidsp:gidsp-api</ignoredNonTestScopedDependency>
            <ignoredNonTestScopedDependency>org.fastable.gidsp:gidsp-service-analytics</ignoredNonTestScopedDependency>
