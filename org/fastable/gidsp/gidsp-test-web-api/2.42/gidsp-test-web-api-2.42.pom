            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-support-jdbc</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-setting</ignoredUnusedDeclaredDependency>
          </ignoredUnusedDeclaredDependencies>
          <ignoredNonTestScopedDependencies>
            
