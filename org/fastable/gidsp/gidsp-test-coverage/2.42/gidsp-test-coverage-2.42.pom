<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.fastable.gidsp</groupId>
    <artifactId>gidsp</artifactId>
    <version>2.42</version>
  </parent>

  <artifactId>gidsp-test-coverage</artifactId>
  <name>gidsp Test Coverage</name>

  <properties>
    <rootDir>../</rootDir>
  </properties>
  
  
  
  <dependencies>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-api</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-schema</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-setting</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-acl</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-audit-consumer</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-node</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-core</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-validation</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-administration</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-dxf2</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-field-filtering</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-metadata-workflow</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-analytics</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-tracker</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-service-reporting</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-support-commons</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-support-db-migration</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-support-external</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-support-hibernate</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-support-audit</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-support-system</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-support-jdbc</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-support-expression-parser</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-support-artemis</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-support-cache-invalidation</artifactId>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-web-api</artifactId>
    </dependency>
    
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-test-web-api</artifactId>
      <version>${project.version}</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.fastable.gidsp</groupId>
      <artifactId>gidsp-test-integration</artifactId>
      <version>${project.version}</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter-api</artifactId>
      <scope>test</scope>
    </dependency>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-dependency-plugin</artifactId>
        <configuration>
          <failOnWarning>true</failOnWarning>
          <ignoredUnusedDeclaredDependencies>
            
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-api</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-schema</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-setting</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-acl</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-audit-consumer</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-node</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-core</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-validation</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-administration</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-dxf2</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-field-filtering</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-metadata-workflow</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-analytics</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-tracker</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-service-reporting</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-support-commons</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-support-db-migration</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-support-external</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-support-hibernate</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-support-audit</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-support-system</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-support-jdbc</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-support-expression-parser</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-support-artemis</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-support-cache-invalidation</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-web-api</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-test-web-api</ignoredUnusedDeclaredDependency>
            <ignoredUnusedDeclaredDependency>org.fastable.gidsp:gidsp-test-integration</ignoredUnusedDeclaredDependency>
            
            <ignoredUnusedDeclaredDependency>org.junit.jupiter:junit-jupiter-api</ignoredUnusedDeclaredDependency>
          </ignoredUnusedDeclaredDependencies>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.jacoco</groupId>
        <artifactId>jacoco-maven-plugin</artifactId>
        <executions>
          <execution>
            <id>report-aggregate</id>
            <goals>
              <goal>report-aggregate</goal>
            </goals>
            <phase>test</phase>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
</project>